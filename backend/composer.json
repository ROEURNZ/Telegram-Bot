{
    "name": "roeurnz/telegram-bot",
    "description": "A PHP-based Telegram bot that interacts with the Telegram API to provide automated responses and functionalities.",
    "type": "library",
    "require": {
        "php": "^8.2|^8.3.10",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.9",
        "google/cloud-speech": "^1.19.0",
        "intervention/image": "^3.8",
        "endroid/qr-code": "^5.1",
        "khanamiryan/qrcode-detector-decoder": "^2.0",
        "picqer/php-barcode-generator": "^2.4",
        "openai-php/client": "^0.10.1",
        "symfony/process": "^6.4.8",
        "php-http/discovery": "^1.19.4",
        "irazasyed/telegram-bot-sdk": "^3.14.0",
        "doctrine/orm": "^3.2.2",
        "doctrine/dbal": "^4.1.1",
        "defuse/php-encryption": "^2.3",
        "vlucas/phpdotenv": "^5.6.1",
        "monolog/monolog": "^3.7.0",
        "respect/validation": "^2.3.7",
        "symfony/cache": "^7.1.4",
        "nikic/fast-route": "^1.3.0",
        "symfony/dotenv": "^7.1.3",
        "pimple/pimple": "^3.5.0",
        "robmorgan/phinx": "^0.16.3",
        "guzzlehttp/psr7": "^2.7.0",
        "longman/telegram-bot": "^0.83.1",
        "google/apiclient": "^2.17.0",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "symfony/intl": "^7.1.1",
        "voku/portable-ascii": "^2.0.1",
        "giggsey/libphonenumber-for-php": "^8.13.45",
        "tecnickcom/tcpdf": "^6.7.5",
        "laminas/laminas-barcode": "^2.14.0",
        "kreativekorp/barcode": "dev-master",
        "libern/qr-code-reader": "^1.0",
        "chillerlan/php-qrcode": "^5.0",
        "tarfin-labs/zbar-php": "^1.7"
        
    },
    "require-dev": {
        "nunomaduro/collision": "^8.4.0",
        "phpunit/phpunit": "^11.3.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\TelegramBot\\": "app/"
        }
    },
    "authors": [
        {
            "name": "ROEURNZ",
            "email": "roeurnkaki@gmail.com",
            "homepage": "https://github.com/ROEURNZ",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}


